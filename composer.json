{
    "name": "santarosa/mvc",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "firebase/php-jwt": "^6.11",
        "php-di/php-di": "^7.0",
        "twig/twig": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload -o --no-dev"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o --no-dev"
        ]
    }
}
